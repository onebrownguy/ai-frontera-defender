    <script>
        // Free Scan Functionality
        function startFreeScan() {
            document.getElementById('scanModal').style.display = 'flex';
        }
        
        function closeScanModal() {
            document.getElementById('scanModal').style.display = 'none';
            document.getElementById('scanResults').style.display = 'none';
            document.getElementById('scanForm').style.display = 'block';
        }
        
        async function submitScan(event) {
            event.preventDefault();
            
            const target = document.getElementById('scanTarget').value;
            const scanType = document.querySelector('input[name="scanType"]:checked').value;
            
            // Show loading
            const submitBtn = event.target.querySelector('button[type="submit"]');
            submitBtn.disabled = true;
            submitBtn.textContent = 'Scanning...';
            
            try {
                const response = await fetch('/api/pentest-demo', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify({ target, scanType })
                });
                
                const data = await response.json();
                
                // Display results
                displayScanResults(data);
                
            } catch (error) {
                console.error('Scan error:', error);
                displayDemoResults();
            } finally {
                submitBtn.disabled = false;
                submitBtn.textContent = 'Start Scanning';
            }
        }
        
        function displayScanResults(data) {
            document.getElementById('scanForm').style.display = 'none';
            document.getElementById('scanResults').style.display = 'block';
            
            const resultsHTML = `
                <div style="background:var(--bg-light); padding:1rem; border-radius:8px; margin-bottom:1rem;">
                    <h4 style="color:var(--primary-dark); margin-bottom:0.5rem;">Security Score</h4>
                    <div style="font-size:2rem; font-weight:bold; color:${data.results?.riskScore?.score > 80 ? 'var(--success-green)' : 'var(--threat-red)'};">
                        ${data.results?.riskScore?.grade || 'B'} (${data.results?.riskScore?.score || 75}/100)
                    </div>
                </div>
                
                <div style="margin-bottom:1rem;">
                    <h4 style="color:var(--primary-dark); margin-bottom:0.5rem;">Vulnerabilities Found</h4>
                    <ul style="list-style:none; padding:0;">
                        ${data.results?.vulnerabilities?.map(v => `
                            <li style="padding:0.5rem; background:var(--bg-light); margin-bottom:0.5rem; border-radius:4px;">
                                <span style="color:${v.severity === 'HIGH' ? 'var(--threat-red)' : 'var(--text-secondary)'}; font-weight:600;">
                                    ${v.severity}:
                                </span> ${v.type}
                            </li>
                        `).join('') || '<li>No critical vulnerabilities found</li>'}
                    </ul>
                </div>
                
                <div style="background:var(--primary-blue); color:white; padding:1rem; border-radius:8px;">
                    <p style="margin-bottom:0.5rem;">Want the full report?</p>
                    <a href="/signup" style="color:white; text-decoration:underline;">Get your complete assessment free →</a>
                </div>
            `;
            
            document.getElementById('resultsContent').innerHTML = resultsHTML;
        }
        
        function displayDemoResults() {
            document.getElementById('scanForm').style.display = 'none';
            document.getElementById('scanResults').style.display = 'block';
            
            document.getElementById('resultsContent').innerHTML = `
                <div style="background:var(--bg-light); padding:1rem; border-radius:8px;">
                    <p>Demo scan completed! For live results, <a href="/signup">sign up for free</a>.</p>
                </div>
            `;
        }
    </script>

